<center>
  <h1>Viewing Party Lite</h1>
  <hr>
  <% if !session[:user_id] %>
    <%= button_to "Login", "/login", method: :get %>
    <%= button_to "Register", "/register", method: :get %>
    <br>
    <% @users.each do |user| %>
      <div class = "<%= user.name %>">
        <%= user.email %>
      </div>
    <% end %>
  <% else %>
    <%= button_to "Logout", "/logout", method: :get %>
  <% end %>
</center>
